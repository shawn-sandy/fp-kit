{
  "name": "fp-kit",
  "version": "0.0.1",
  "private": true,
  "description": "A UI toolkit for creating and managing style-guides, design systems, pattern libraries, apps, digital products, etc.",
  "scripts": {
    "start": "start-storybook -p 6006",
    "test": "jest",
    "serve": "eleventy --serve",
    "builds": "run-s build:*",
    "changelog": "auto-changelog -p --template config/_changelog.hbs",
    "audit": "lighthouse https://shawnsandy.com/ --output=json --output-path=./audits/report.json --config-path=.lighthouseconfig.js",
    "build:first-paint": "lerna exec npm run build --scope=@shawnsandy/first-paint",
    "npm-next": "lerna publish --dist-tag next patch --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish": "lerna publish patch --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish:minor": "lerna publish minor --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish:major": "lerna publish major --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "release-patch": "lerna version patch --conventional-commits --no-private",
    "release-minor": "lerna version minor --conventional-commits --no-private",
    "release": "lerna version major --conventional-commits --no-private",
    "git:add": "git add .",
    "git:cz": "git cz",
    "git:push": "git push",
    "commit": "run-s git:add git:cz git:push",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "eslint": "eslint '**/src/**/*.jsx'",
    "lint-staged": "lint-staged",
    "commitlint": "npx commitlint",
    "mit": "mitosis compile --to=react src/component.lite.jsx > components/react/components/component.jsx",
    "storybook": "start-storybook -p 9006",
    "storybook-test": "test-storybook --url http://localhost:9006",
    "storybook-test:watch": "test-storybook --url http://localhost:9006 --watch --coverage",
    "build-storybook": "build-storybook",
    "test:storybook": "storybook.test.js",
    "oc": "git add . && oc"
  },
  "auto-changelog": {
    "unreleased": true,
    "template": "config/_changelog.hbs"
  },
  "main": "index.js",
  "repository": "https://github.com/shawn-sandy/fp-kit.git",
  "author": "shawnsandy <shawnsandy04@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@azimutlabs/eslint-config-jsx-a11y": "^1.1.1",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@dgonzalezr/storybook-addon-html": "^5.0.0",
    "@geometricpanda/storybook-addon-badges": "^0.2.2",
    "@mdx-js/react": "^1.6.22",
    "@nrwl/cypress": "15.5.1",
    "@nrwl/jest": "15.5.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "15.5.1",
    "@nrwl/web": "15.5.1",
    "@shawnsandy/first-paint": "^2.17.32",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-coverage": "^0.0.4",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-jest": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addon-storyshots": "^6.5.15",
    "@storybook/addon-storysource": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/jest": "^0.0.10",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/mdx2-csf": "^0.0.3",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.16",
    "@storybook/test-runner": "^0.9.3",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/testing-react": "^1.3.0",
    "@testing-library/react": "13.4.0",
    "@types/jest": "^29.4.0",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "5.3.3",
    "auto": "^10.37.1",
    "auto-changelog": "^2.4.0",
    "babel-jest": "^28.1.0",
    "babel-loader": "^8.2.5",
    "conventional-changelog-atom": "^2.0.8",
    "css-loader": "^6.7.3",
    "cypress": "^9.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.4",
    "eslint-plugin-vue": "^9.0.1",
    "flat-cache": "^3.0.4",
    "fs-extra": "^10.1.0",
    "hint": "^6.2.0",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.0",
    "hygen": "^6.2.0",
    "jest": "^28.1.0",
    "lerna": "^6.4.1",
    "lerna-script": "^1.4.0",
    "lighthouse": "^9.6.8",
    "lint-staged": "^12.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "nx": "^15.5.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "standard": "^17.0.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-code-panel": "^0.2.1",
    "style-dictionary": "^3.7.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "trash-cli": "^5.0.0",
    "ts-jest": "^28.0.3",
    "ts-node": "10.8.0",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "tslib": "^2.4.0"
  },
  "dependencies": {
    "@fpkit/nextjs": "^0.0.14",
    "chromatic": "^6.17.3",
    "core-js": "^3.22.7",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "regenerator-runtime": "0.13.9",
    "storybook-addon-next": "^1.7.1"
  },
  "lint-staged": {
    "**/.*.{ts,jsx,tsx}": "tsc --noEmit",
    "**/.*.{js,ts,jsx,tsx}": "eslint --fix",
    "**/.*.{js,jsx,ts,tsx,md,html,scss,json,md}": "prettier --write",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "auto": {
    "plugins": [
      "npm"
    ],
    "onlyPublishWithReleaseLabel": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "postcss": "8.4.19"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}