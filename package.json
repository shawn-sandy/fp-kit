{
  "name": "fp-kit",
  "version": "0.0.1",
  "private": true,
  "description": "A UI toolkit for creating and managing style-guides, design systems, pattern libraries, apps, digital products, etc.",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "start": "docsify serve docs",
    "serve": "eleventy --serve",
    "eleventy": "run-s eleventy --serve",
    "build": "npx @11ty/eleventy",
    "builds": "run-s build:*",
    "changelog": "auto-changelog -p --template config/_changelog.hbs",
    "audit": "lighthouse https://shawnsandy.com/ --output=json --output-path=./audits/report.json --config-path=.lighthouseconfig.js",
    "build:first-paint": "lerna exec npm run build --scope=@shawnsandy/first-paint",
    "npm-release": "HUSKY=0 && lerna publish --conventional-commits -m \"chore(release): publish patch \"",
    "npm-next": "lerna publish --dist-tag next patch --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish": "HUSKY=0 && lerna publish --yes  --dist-tag next --conventional-commits -m 'chore(next): publish @next %s'",
    "release-patch": "lerna version patch --conventional-commits",
    "release-minor": "lerna version minor --conventional-commits",
    "release": "lerna version major --conventional-commits",
    "git:add": "git add .",
    "git:cz": "git cz",
    "git:push": "git push",
    "commit": "run-s git:add git:cz git:push",
    "prepare": "husky install",
    "plop": "plop",
    "eslint": "eslint '**/src/**/*.jsx'",
    "lint-staged": "lint-staged",
    "commitlint": "npx commitlint",
    "mit": "mitosis compile --to=react src/component.lite.jsx > components/react/components/component.jsx",
    "laddle": "ladle serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "directories": {
    "lib": "dist",
    "gulp": "gulp",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "gulp"
  ],
  "auto-changelog": {
    "unreleased": true,
    "template": "config/_changelog.hbs"
  },
  "main": "index.js",
  "repository": "https://github.com/shawn-sandy/fp-kit.git",
  "author": "shawnsandy <shawnsandy04@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.1",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-navigation": "^0.3.3",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@azimutlabs/eslint-config-jsx-a11y": "^1.1.1",
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@ladle/react": "^0.16.0",
    "@nrwl/cypress": "14.1.7",
    "@nrwl/jest": "14.1.7",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "14.1.7",
    "@nrwl/web": "14.1.7",
    "@shawnsandy/bootstrap-lite": "^0.2.17",
    "@shawnsandy/first-paint": "^2.1.4",
    "@shawnsandy/ideas": "^0.3.12",
    "@shawnsandy/mix": "^0.10.32",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/react": "13.2.0",
    "@types/jest": "27.5.1",
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "5.3.3",
    "auto": "^10.37.1",
    "auto-changelog": "^2.4.0",
    "babel-jest": "28.1.0",
    "babel-loader": "^8.2.5",
    "conventional-changelog-atom": "^2.0.8",
    "cypress": "^9.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-plugin-vue": "^9.0.1",
    "flat-cache": "^3.0.4",
    "fs-extra": "^10.1.0",
    "hint": "^6.2.0",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.1",
    "hygen": "^6.2.0",
    "jest": "28.1.0",
    "lerna": "^5.1.4",
    "lerna-script": "^1.4.0",
    "lighthouse": "^9.6.1",
    "lint-staged": "^12.4.2",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.4",
    "npm-run-all": "^4.1.5",
    "nx": "^14.1.9",
    "parcel": "^2.6.0",
    "plop": "latest",
    "pre-commit": "^1.2.2",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "18.1.0",
    "require-dir": "^1.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.52.1",
    "standard": "^17.0.0",
    "style-dictionary": "^3.7.0",
    "stylelint": "^14.8.5",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.2.0",
    "trash-cli": "^5.0.0",
    "ts-jest": "28.0.3",
    "ts-node": "10.8.0"
  },
  "lint-staged": {
    "**/.*.{js,ts,jsx,tsx}": "eslint --fix",
    "**/.*.{js,jsx,ts,tsx,md,html,scss,json,md}": "prettier --write"
  },
  "auto": {
    "plugins": [
      "npm"
    ],
    "onlyPublishWithReleaseLabel": true
  },
  "dependencies": {
    "@builder.io/mitosis": "^0.0.55",
    "core-js": "^3.22.7",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0",
    "regenerator-runtime": "0.13.9",
    "tslib": "^2.4.0",
    "vite-plugin-dts": "^1.2.0"
  }
}
