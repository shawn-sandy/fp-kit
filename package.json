{
  "name": "fp-kit",
  "version": "0.0.1",
  "private": true,
  "description": "A UI toolkit for creating and managing style-guides, design systems, pattern libraries, apps, digital products, etc.",
  "scripts": {
    "start": "npm run storybook",
    "test": "jest",
    "serve": "eleventy --serve",
    "builds": "run-s build:*",
    "changelog": "auto-changelog -p --template config/_changelog.hbs",
    "audit": "lighthouse https://shawnsandy.com/ --output=json --output-path=./audits/report.json --config-path=.lighthouseconfig.js",
    "build:first-paint": "lerna exec npm run build --scope=@shawnsandy/first-paint",
    "npm-next": "lerna publish --dist-tag next patch --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish": "lerna publish --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish:minor": "lerna publish minor --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "npm-publish:major": "lerna publish major --no-private --conventional-commits -m \"chore(next): publish next release \"",
    "release-patch": "lerna version patch --conventional-commits --no-private",
    "release-minor": "lerna version minor --conventional-commits --no-private",
    "release": "lerna version major --conventional-commits --no-private",
    "git:add": "git add .",
    "git:cz": "git cz",
    "git:push": "git push",
    "commit": "run-s git:add git:cz git:push",
    "prepare": "chmod +x ./node_modules/husky/lib/bin.js && husky install",
    "eslint": "eslint '**/src/**/*.jsx'",
    "commitlint": "npx commitlint",
    "mit": "mitosis compile --to=react src/component.lite.jsx > components/react/components/component.jsx",
    "chromatic": "chromatic --exit-zero-on-changes",
    "chromatic:dry-run": "chromatic --dry-run",
    "storybook": "storybook dev -p 9006",
    "storybook-test": "test-storybook --url http://localhost:9006",
    "storybook-test:watch": "test-storybook --url http://localhost:9006 --watch --coverage",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook -- --coverage ",
    "storybook:snapshot": "test-storybook -- u",
    "oco": "git add . && oc",
    "aicommits": "aicommits -a --type conventional"
  },
  "auto-changelog": {
    "unreleased": true,
    "template": "config/_changelog.hbs"
  },
  "main": "index.js",
  "repository": "https://github.com/shawn-sandy/fp-kit.git",
  "author": "shawnsandy <shawnsandy04@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@azimutlabs/eslint-config-jsx-a11y": "^1.1.1",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@chromaui/addon-visual-tests": "^0.0.105",
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@dgonzalezr/storybook-addon-html": "^5.0.0",
    "@geometricpanda/storybook-addon-badges": "^2.0.1",
    "@mdx-js/react": "^2.3.0",
    "@nrwl/cypress": "^16.10.0",
    "@nrwl/jest": "^16.10.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "^16.10.0",
    "@nrwl/web": "^16.10.0",
    "@shawnsandy/first-paint": "^2.18.8",
    "@storybook/addon-a11y": "^7.6.8",
    "@storybook/addon-actions": "^7.6.8",
    "@storybook/addon-coverage": "^1.0.0",
    "@storybook/addon-docs": "^7.6.8",
    "@storybook/addon-essentials": "^7.6.8",
    "@storybook/addon-interactions": "^7.6.8",
    "@storybook/addon-jest": "^7.6.8",
    "@storybook/addon-links": "^7.6.8",
    "@storybook/addon-mdx-gfm": "^7.6.8",
    "@storybook/addon-queryparams": "^6.2.9",
    "@storybook/addon-storyshots": "^7.6.8",
    "@storybook/addon-storysource": "^7.6.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/mdx2-csf": "^1.1.0",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^7.6.8",
    "@storybook/react-webpack5": "^7.6.8",
    "@storybook/test-runner": "^0.16.0",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/testing-react": "^2.0.1",
    "@testing-library/react": "14.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.4",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-router-dom": "5.3.3",
    "auto": "^11.0.4",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "conventional-changelog-atom": "^4.0.0",
    "css-loader": "^6.8.1",
    "cssnano": "^6.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-vue": "^9.17.0",
    "flat-cache": "^3.1.1",
    "fs-extra": "^11.1.1",
    "hint": "^7.1.10",
    "html-minifier": "^4.0.0",
    "husky": "^8.0.3",
    "hygen": "^6.2.11",
    "jest": "^29.7.0",
    "lerna": "^7.3.1",
    "lerna-script": "^1.4.0",
    "lighthouse": "^11.2.0",
    "lint-staged": "^14.0.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "npm-run-all": "^4.1.5",
    "nx": "^16.10.0",
    "postcss-import": "^15.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "require-dir": "^1.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.3",
    "sass-loader": "^13.3.2",
    "standard": "^17.1.0",
    "storybook": "^7.6.8",
    "storybook-addon-react-docgen": "^1.2.44",
    "storybook-addon-sass-postcss": "^0.1.3",
    "storybook-code-panel": "^0.2.1",
    "style-dictionary": "^3.8.0",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.0.00",
    "stylelint-scss": "^5.2.1",
    "trash-cli": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "10.9.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "tslib": "^2.6.2"
  },
  "dependencies": {
    "@fpkit/nextjs": "^0.2.5",
    "@storybook/react-vite": "^7.6.8",
    "chromatic": "^7.3.0",
    "core-js": "^3.33.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "^6.16.0",
    "regenerator-runtime": "^0.14.0",
    "storybook-addon-next": "^1.8.0",
    "vite": "^4.4.11"
  },
  "lint-staged": {
    "**/.*.{ts,jsx,tsx}": "tsc --noEmit",
    "**/.*.{js,ts,jsx,tsx}": "eslint --fix",
    "**/.*.{js,jsx,ts,tsx,md,html,scss,json,md}": "prettier --write",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "auto": {
    "plugins": [
      "npm"
    ],
    "onlyPublishWithReleaseLabel": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "overrides": {
    "@mdx-js/react": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    },
    "postcss": "8.4.19"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ]
}
