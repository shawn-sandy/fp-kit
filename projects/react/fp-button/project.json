{
  "$schema": "..\\..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "projects/react/fp-button/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/projects/react/fp-button",
        "tsConfig": "projects/react/fp-button/tsconfig.lib.json",
        "project": "projects/react/fp-button/package.json",
        "entryFile": "projects/react/fp-button/src/index.ts",
        "external": ["react/jsx-runtime"],
        "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "projects/react/fp-button/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "projects/react/fp-button/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/projects/react/fp-button"],
      "options": {
        "jestConfig": "projects/react/fp-button/jest.config.ts",
        "passWithNoTests": true
      }
    }
  }
}
